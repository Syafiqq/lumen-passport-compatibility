        "php": "^7.1.3",
        "laravel/lumen-framework": "5.8.*",
        "laravel/passport": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.4"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "Syafiqq\\Passport\\": "src/"
        }
    },
    "config": {
