{
    "name": "syafiqq/lumen-framework-extended",
    "description": "Lumen - Passport Compatibility Patches.",
    "keywords": ["lumen", "passport", "oauth"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://lumen.laravel.com",
    "support": {
        "issues": "https://github.com/laravel/passport/issues",
        "source": "https://github.com/laravel/passport"
    },
    "authors": [
        {
            "name": "Syafiqq",
            "email": "syafiq.rezpector@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
